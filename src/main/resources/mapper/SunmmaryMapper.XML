<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sicnu.consystem.Mapper.SunmmaryMapper">

    <insert id="addSunmmary" useGeneratedKeys="true" keyProperty="id" parameterType="com.sicnu.consystem.Pojo.Sunmmary">
        insert into sunmmary (mid,content) values (#{mid},#{content});
    </insert>
    <update id="updateSunmmary">
        update sunmmary set content=#{content} where mid=#{mid}
    </update>
    <select id="getSunmmary" resultType="com.sicnu.consystem.Pojo.Sunmmary">
        select * from sunmmary where mid=#{mid}
    </select>
</mapper>
